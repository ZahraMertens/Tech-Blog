<!-- Loop over each object in the projects array -->
{{#each posts as |post|}}
    
    <div class="card text-center">
     <a href="/post/{{post.id}}">
      <div class="card-body">
        <h5 class="card-title">{{post.title}}</h5>
      </div>
      </a>
      <div class="card-footer text-muted">
        Created by {{post.user.username}} on {{format_date post.date_created}}
      </div>
    </div>
{{/each}}
